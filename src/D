-    refIndent = minIndent;
-  else
-    refIndent = 0;
+  if (minIndent!=maxIndent) refIndent=minIndent; else refIndent=0;
   out.addChar(0);
-  // printf("detab refIndent=%d\n",refIndent);
+  //printf("detab refIndent=%d\n",refIndent);
   return out.get();
 }
 
