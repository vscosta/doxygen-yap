+          cache.arguments.reset(ArgumentListContext::alloc(defArgList,m_memberDef,relPathAsString()));
+        }
+        else
+        {
+          cache.arguments.reset(ArgumentListContext::alloc());
+        }
+      }
+      return cache.arguments.get();
+    }
+    TemplateVariant hasParameters() const
+    {
