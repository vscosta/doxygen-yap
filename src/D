-  case Entry::PROTOCOLDOC_SEC:
-    sec = ClassDef::Protocol;
-    break;
-  // case Entry::CATEGORY_SEC:
-  case Entry::CATEGORYDOC_SEC:
-    sec = ClassDef::Category;
-    break;
-  // case Entry::EXCEPTION_SEC:
-  case Entry::EXCEPTIONDOC_SEC:
-    sec = ClassDef::Exception;
-    break;
